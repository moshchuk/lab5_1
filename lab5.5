#include <iostream>

using namespace std;

int f(int n) {
    if (n == 0) {
        return 0;
    }
    if (n % 10 > 0) {
        return n % 10;
    }
    return f(n / 10);
}

int S(int p, int q) {
    int S = 0;
    for (int i = p; i <= q; ++i) {
        S+= f(i);
    }
    return S;
}

